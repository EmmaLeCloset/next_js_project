export async function getProducts() {
  // const res = await fetch(`${process.env.NEXT_PUBLIC_API_URL}/api/catalog/browsing?type=&availability=all&kind=women&page=undefined`);
  const res = await fetch(`http://localhost:3004/api/catalog/browsing?type=&availability=all&kind=women&page=undefined`);
  console.log('url', process.env.NEXT_PUBLIC_API_URL);

  console.log('products', res);
  return res.json();
}


export async function getProduct(queryString) {
  // const res = await fetch(`${process.env.NEXT_PUBLIC_API_URL}/api/catalog/browsing?type=&availability=all&kind=women&page=undefined`);
  const res = await fetch(`http://localhost:3004/api/catalog/browsing/${queryString}`);
  console.log('url', `${process.env.NEXT_PUBLIC_API_URL}`);

  console.log('products', res);
  return res.json();
}
